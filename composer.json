{
    "name": "dbtedman/extremity",
    "autoload": {
        "psr-4": {
            "DBTedman\\Extremity\\": "src/php/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DBTedman\\ExtremityTest\\": "test/unit/"
        }
    },
    "authors": [
        {
            "name": "Daniel Tedman",
            "email": "dbtedman@gmail.com"
        }
    ],
    "require": {
        "php": "7.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.11",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "3.6.2"
    },
    "scripts": {
        "format": "phpcbf",
        "lint": "phpcs -p",
        "test": "phpunit"
    }
}
